<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="400.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="Controllers.TableController">
	<children>
		<Label layoutX="240.0" layoutY="174.0" />
		<Button layoutX="86.0" layoutY="60.0" mnemonicParsing="false"
			onAction="#getTable" prefHeight="39.0" prefWidth="210.0"
			text="Get Tabel Information" />
		<Button layoutX="84.0" layoutY="141.0" mnemonicParsing="false"
			onAction="#addTable" prefHeight="39.0" prefWidth="210.0"
			text="Add a New Table" />
		<Button layoutX="85.0" layoutY="228.0" mnemonicParsing="false"
			onAction="#updateTableCapacity" prefHeight="39.0" prefWidth="210.0"
			text="Update a Table" />
		<Button layoutX="82.0" layoutY="306.0" mnemonicParsing="false"
			onAction="#deleteTable" prefHeight="39.0" prefWidth="210.0"
			text="Delete a Table" />
	</children>
</AnchorPane>
